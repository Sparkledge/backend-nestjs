datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URI")
}

generator client {
  provider = "prisma-client-js"
}

model User{
     id       String    @id @default(auto()) @map("_id") @db.ObjectId
     firstName String
     lastName String
     password String
     //temporaryToken String @default(null)
     //verified Boolean @default(false)
     //refreshToken String @default(null)

}